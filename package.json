    "concurrently": "^3.5.0",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "express": "^4.15.4",
    "jsonwebtoken": "^8.2.2",
    "mongoose": "^5.1.4",
    "passport": "^0.4.0"
  }
